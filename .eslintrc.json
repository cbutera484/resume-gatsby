{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "no-cond-assign": [2, "always"],
    "no-console": 1,
    "no-constant-condition": 2,
    "no-debugger": 1,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-extra-parens": [2, "all"],
    "no-extra-semi": 2,
    "no-inner-declarations": [2, "both"],
    "no-irregular-whitespace": 2,
    "no-unreachable": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-alert": 1,
    "no-caller": 1,
    "no-eq-null": 2,
    "no-eval": 1,
    "no-fallthrough": 1,
    "no-loop-func": 2,
    "no-redeclare": 2,
    "no-return-assign": [2, "always"],
    "no-self-compare": 1,
    "no-useless-call": 2,
    "no-void": 2,
    "radix": 2,
    "vars-on-top": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "array-bracket-spacing": [
      2,
      "always",
      { "objectsInArrays": true, "arraysInArrays": true }
    ],
    "brace-style": [2, "1tbs", {}],
    "camelcase": [2, { "properties": "always" }],
    "comma-spacing": [2, { "after": true }],
    "comma-style": 2,
    "eol-last": 1,
    "func-style": [2, "expression"],
    "indent": [2, "tab"],
    "key-spacing": [2, { "afterColon": true }],
    "new-cap": [1, { "newIsCap": true, "capIsNew": true }],
    "new-parens": 2,
    "newline-after-var": [2, "always"],
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-nested-ternary": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2, "always", {}],
    "quotes": [2, "backtick"],
    "semi-spacing": [2, { "after": true }],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "always"],
    "space-in-parens": [2, "always", { "exceptions": ["{}", "[]", "()"] }],
    "arrow-spacing": [2, { "before": true, "after": true }],
    "constructor-super": 2,
    "no-var": 2,
    "prefer-const": 2,
    "prefer-spread": 2
  }
}
